<Window x:Class="RecipeAppPOE.ViewRecipe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RecipeAppPOE"
        mc:Ignorable="d"
        Title="ViewRecipe" Height="450" Width="800">
    <Grid x:Name="viewRecipeWindow">

        <Grid.Background>
            <ImageBrush ImageSource="https://i.pinimg.com/originals/c1/14/c3/c114c345413eebe514d073473e96c756.jpg"/>
        </Grid.Background>
        <Label x:Name="viewRecipesLbl" Content="Recipes:" HorizontalAlignment="Left" Margin="124,10,0,0" VerticalAlignment="Top" Foreground="White" FontSize="36"/>
        <Label x:Name="displayRecipesLbl" Content="Label" HorizontalAlignment="Left" Margin="37,73,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>


    </Grid>
</Window>
